{
    "name": "@quatico/magellan-test",
    "description": "End to end tests for magellan suite",
    "version": "0.1.4",
    "author": "Quatico Solutions AG",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "license": "SEE LICENSE IN LICENSE",
    "private": true,
    "files": [
        "LICENSE",
        "README.md",
        "lib"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "clean": "rimraf lib coverage output",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix --color",
        "build": "tsc",
        "watch": "tsc --watch",
        "test:update-snapshots": "yarn test -u",
        "test:watch": "jest --color --watch",
        "test:e2e": "./node_modules/.bin/cucumber-js"
    },
    "devDependencies": {
        "@types/node": "^16.0.0",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.1.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-testing-library": "^5.9.1",
        "memfs": "~3.4.11",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "@cucumber/cucumber": "^8.7.0",
        "@quatico/magellan-cli": "0.1.4",
        "@quatico/magellan-client": "0.1.4",
        "@quatico/magellan-server": "0.1.4",
        "@quatico/magellan-shared": "0.1.4",
        "typescript": "~4.7.4"
    },
    "engines": {
        "node": ">=16.x",
        "yarn": ">=1.20.x"
    }
