    "license": "SEE LICENSE IN LICENSE",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/quatico-solutions/magellan.git"
    },
    "bugs": {
