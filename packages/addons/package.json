        "cross-env": "7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^26.1.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-testing-library": "^5.2.0",
